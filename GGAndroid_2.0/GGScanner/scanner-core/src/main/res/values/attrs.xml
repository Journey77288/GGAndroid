<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="zbar_view">
        <attr name="scan_bg_color" format="reference" />
        <attr name="scan_rect_width" format="dimension" />
        <attr name="scan_rect_height" format="dimension" />
        <attr name="scan_rect_margin_top" format="dimension" />
        <attr name="scan_corner_color" format="color" />
        <attr name="scan_corner_width" format="dimension" />
        <attr name="scan_corner_size" format="dimension" />
        <attr name="scan_line_height" format="dimension" />
        <attr name="scan_line_width" format="dimension" />
        <attr name="scan_line_color" format="dimension" />
        <attr name="scan_line_margin_left" format="dimension" />
        <attr name="scan_line_margin_right" format="dimension" />
        <attr name="scan_line_margin_top" format="dimension" />
        <attr name="scan_line_margin_bottom" format="dimension" />
        <attr name="scan_qr_type" format="integer">
            <enum name="TYPE_QR_CODE" value="1" />
            <enum name="TYPE_BAR_CODE" value="2" />
            <enum name="TYPE_ALL" value="3" />
            <enum name="TYPE_CUSTOM" value="4" />
        </attr>

    </declare-styleable>
</resources>